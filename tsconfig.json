{
  "compilerOptions": {
    "esModuleInterop": true,
    "jsx": "react",
    "module": "esnext",
    "moduleResolution": "node",
    "lib": [
      "dom",
      "esnext"
    ],
    "baseUrl": ".",
    "paths": {
      "src": ["src"]
    },
    "strict": true,
    "sourceMap": true,
    "target": "esnext",
    "types": ["src/raw-loader.d.ts", "node"],
    "allowSyntheticDefaultImports": true
  },
  "exclude": [
    "node_modules"
    ]
}